package ex02_casting;

public class Ex03 {

	public static void main(String[] args) {

		// DataBase로 보낼때 100으로 보내는게 아니라 "100"으로 보내서, 100으로 변환하는 과정.
		String strScore = "100";
		String strMoney = "10000000000";
		String strGrade = "4.5";
		String a = "121";
		
		// 문자열을 숫자데이터로 변환하기
		// 아래 변환은 매우 중요하다.
		int score = Integer.parseInt(strScore);
		
		Integer.parseInt(a);
		
		
		System.out.println(strScore);
		System.out.println(score);
		System.out.println(a);
		
		

	}

}
